          "base_uri": "https://localhost:8080/",
          "height": 68
        }
      },
      "source": [
        "# We split the abstract using different tokenizer and see how large the vocabulary is \n",
        "\n",
        "from nltk.tokenize import TreebankWordTokenizer\n",
        "from nltk.tokenize import ToktokTokenizer\n",
        "from nltk.tokenize import TweetTokenizer\n",
        "\n",
        "tokenizers = [TreebankWordTokenizer(), ToktokTokenizer(), TweetTokenizer()]\n",
        "\n",
        "for tok in tokenizers:\n",
        "  arr = df.verified_reviews.drop_duplicates().apply(lambda x: tok.tokenize(x)).array\n",
        "  arr = reduce(add, arr)\n",
        "  # the set object transforms a list to the set of unique elements in the list :\n",
        "  print(\"Vocabulary size: {0} with {1} tokenizer\".format(len(set(arr)), tok))  "
      ],
      "execution_count": 12,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
